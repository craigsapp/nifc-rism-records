  <marc:datafield tag="245" ind1="1" ind2="0">
    <marc:subfield code="a">[cover title:] Concerto De Resurrectione D.[omini] N.[ostri] J.[esu] C.[hristi] | Christus Resurgens ex mortuis | 1 - Canto | 2 - Canto | 1 - Basso | 2 Clarini | A[utore?] Casimiri M BOcz[?] | Mp[?] | 1700</marc:subfield>
  </marc:datafield>
  <marc:datafield tag="260" ind1=" " ind2=" ">
